// out: css/transfer.css
.api-form-version
    position: absolute;
    top:8px;
    right:8px;
    color:#ccc;
    font-size: 8px;
    font-family: 'Lucida Consolas';
iframe.pne-frame
    border:none;
    width:100%;
    height: 480px;
.pne-frame
    position: relative;
    .pne-clicker
        position: absolute;
        top:0;
        left:0;
        width: 100%;
        height: 100%;
        z-index: 100;
    iframe
        border:none;
        width:100%;
        height: 480px;
.pne-frame.pne-frame-default
    iframe *
        pointer-events: none;
.response
    display: none;
    .message
        margin: 40px 0;
        font-size: 28px;
        text-align: center;
        color: #4c79ec;
.response.success
    display:block;
.response.failed
    display:block;
    .message
        color: rgba(255,0,0,0.6);
        text-align: center;
        font-size: 20px;
.alert
    position: absolute;
    top: 24px;
    left: 8px;
    color:red;
    min-width: 620px;
    font-size: 90%;
.input-wrap, .columns
    position: relative;
    .alert
        top:64px;
.checkboxTb
    .alert
        left:32px;
